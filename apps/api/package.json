{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrations:create": "typeorm-ts-node-commonjs -d src/data-source.ts migrations:create -n",
    "migrations:generate": "typeorm-ts-node-commonjs -d src/data-source.ts migrations:generate -n",
    "migrations:run": "ts-node -r tsconfig-paths/register -r dotenv/config src/run-migrations.ts",
    "migrations:run:prod": "node dist/run-migrations.js",
    "migrations:revert": "typeorm-ts-node-commonjs -d src/data-source.ts migrations:revert",
    "db:seed": "ts-node -r tsconfig-paths/register -r dotenv/config src/seeds/seed-defaults.ts",
    "db:setup": "ts-node -r tsconfig-paths/register -r dotenv/config src/seeds/setup-db.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "bash ./test/run-e2e-auto.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/lib-storage": "^3.962.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/storage": "^7.18.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cron": "^4.3.5",
    "express": "^5.2.1",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^5.3.3",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "minimatch": "^10.1.1",
    "pg": "^8.16.3",
    "redlock": "5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "tar": "^7.5.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^2.4.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/redlock": "^4.0.8",
    "@types/supertest": "^6.0.2",
    "@types/tar": "^6.1.13",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
